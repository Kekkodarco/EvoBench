package utente.personale;

import static org.junit.Assert.*;




import listener.BaseCoverageTest;
import listener.JacocoCoverageListener;
import org.junit.Before;
import org.junit.Rule;
import org.junit.Test;
import org.junit.runner.RunWith;


public class AmministratoreTest extends BaseCoverageTest {

    @Test
public void testGetName() {
Amministratore amministratore = new Amministratore("John", "Doe", "HR");
assertEquals("John", amministratore.getName());
    }

    @Test
public void testGetSurname() {
Amministratore amministratore = new Amministratore("John", "Doe", "HR");
assertEquals("Doe", amministratore.getSurname());
    }



    @Test
public void testSetSurname() {
Amministratore amministratore = new Amministratore("John", "Doe", "HR");
amministratore.setSurname("Foe");
assertEquals("Foe", amministratore.getSurname());
    }

    @Test
public void testSetName_case1() {
        // Arrange
String name = "John";
String surname = "Doe";
String department = "IT";
Amministratore amministratore = new Amministratore(name, surname, department);

        // Act
amministratore.setName("Jane");

        // Assert
assertEquals("Jane", amministratore.getName());
    }
    @Test
public void testSetName_case2() {
        // Arrange
String name = "John";
String surname = "Doe";
String department = "IT";
Amministratore amministratore = new Amministratore(name, surname, department);

        // Act
amministratore.setName(null);

        // Assert
assertNull(amministratore.getName());
    }
    @Test
public void testSetName_case3() {
        // Arrange
String name = "John";
String surname = "Doe";
String department = "IT";
Amministratore amministratore = new Amministratore(name, surname, department);

        // Act
amministratore.setName("");

        // Assert
assertEquals("", amministratore.getName());
    }
























    @Test
public void testGetDepartment_NullInput_case1() {
Amministratore amministratore = new Amministratore("John", "Doe", null);
assertEquals(null, amministratore.getDepartment());
    }
    @Test
public void testGetDepartment_ValidInput_case1() {
Amministratore amministratore = new Amministratore("John", "Doe", "IT");
assertEquals("IT", amministratore.getDepartment());
    }
    @Test
public void testSetDepartment_NullInput_case1() {
Amministratore amministratore = new Amministratore("John", "Doe", null);
amministratore.setDepartment(null);
assertEquals(null, amministratore.getDepartment());
    }
    @Test
public void testSetDepartment_ValidInput_case1() {
Amministratore amministratore = new Amministratore("John", "Doe", null);
amministratore.setDepartment("IT");
assertEquals("IT", amministratore.getDepartment());
    }






}
