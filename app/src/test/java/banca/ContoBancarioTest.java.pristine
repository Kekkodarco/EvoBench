package banca;

import static org.junit.Assert.*;







import org.junit.Assert;
import org.junit.Test;
import org.junit.Before;
import listener.BaseCoverageTest;

public class ContoBancarioTest  extends BaseCoverageTest {

    @Before
public void setUp() {
contobancario = new ContoBancario();
    }



private ContoBancario contobancario;

    @Test
public void prelievo_saldoNegativo_case1() {
        // Test case: prelievo from a negative balance
ContoBancario conto = new ContoBancario("1234", -10);
int quota = 5;
Assert.assertEquals(0, conto.prelievo(quota));
    }

    @Test
public void prelievo_saldoSufficiente_case1() {
        // Test case: prelievo from a sufficient balance
ContoBancario conto = new ContoBancario("1234", 10);
int quota = 5;
Assert.assertEquals(1, conto.prelievo(quota));
    }

    @Test
public void testHasSaldoPositivo_case1() {
        // Arrange
ContoBancario c = new ContoBancario("123456", 0);

        // Act
boolean result = c.hasSaldoPositivo();

        // Assert
assertFalse(result);
    }







}
